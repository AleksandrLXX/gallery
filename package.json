{
  "name": "gallery",
  "private": true,
  "version": "0.0.1",
  "description": "Gallery in react ecosystem",
  "main": "",
  "scripts": {
    "babel": "babel --presets react,es2015,stage-0 --ignore node_modules,dist",
    "babel-node": "babel-node --presets react,es2015,stage-0 --ignore node_modules,dist",
    "clean": "rimraf dist/assets && rimraf compiled",
    "start": "NODE_ENV=production node compiled/index.js",
    "dev": "NODE_ENV=development nodemon",
    "build:dev": "NODE_ENV=development webpack --colors --config ./webpack/webpack-dev-server.js",
    "build:prod": "NODE_ENV=production webpack --colors --config ./webpack/webpack-prod.js",
    "build:node": "NODE_ENV=production npm run babel -- ./server -d ./compiled && cp -r ./server/data ./compiled",
    "build": "npm run clean && npm run build:prod && npm run build:node",
    "test": "karma start",
    "test:watch": "karma start --autoWatch=true --singleRun=false",
    "posttest": "npm run lint",
    "serve": "node server.js --env=dev",
    "serve:dist": "node server.js --env=dist",
    "dist": "npm run copy & webpack --env=dist",
    "lint": "eslint client server",
    "copy": "copyfiles -f ./src/index.html ./src/favicon.ico ./dist",
    "release:major": "npm version major && npm publish && git push --follow-tags",
    "release:minor": "npm version minor && npm publish && git push --follow-tags",
    "release:patch": "npm version patch && npm publish && git push --follow-tags"
  },
  "repository": "",
  "keywords": [],
  "author": "Your name here",
